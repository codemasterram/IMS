@model SoftIms.Data.ViewModel.DepreciationRateViewModel
<div class="col-md-12 ">
    @using (Html.BeginForm("CreateDepreciationRate", "master", FormMethod.Post, new { @enctype = "multipart/form-data", @class = "form-inline smooth-tab" }))
            {
        @Html.HiddenFor(x => x.Id)
        @Html.HiddenFor(x => x.ItemGroupId)
        @Html.AntiForgeryToken()
        @Html.Hidden("next", "family-detail-tab")
        <div class="form-group col-md-4">
            @Html.LabelFor(x => x.FiscalYearId, new { @class = "col-md-6" })
            <div>
                @Html.DropDownListFor(x => x.FiscalYearId, MasterDataList<FiscalYear>(selectedValue: Model.FiscalYearId), new { @class = "form-control col-md-3" })
            </div><div>
                @Html.ValidationMessageFor(x => x.FiscalYearId)
            </div>
        </div>
        <div class="form-group col-md-5">
            @Html.LabelFor(x => x.Rate, new { @class = "col-md-3" })
            <div>
                @Html.TextBoxFor(x => x.Rate, new { @class = "form-control col-md-6 number-type amount" })
                @Html.ValidationMessageFor(x => x.Rate)
            </div>
        </div>
        <div class="form-group col-md-3">
            @*<div class="modal-footer">*@
                <button type="submit" class="btn btn-success">Submit</button>
            @*</div>*@
        </div>

    }                                                                   2112
</div>
<script>
    initializeCommon();
</script>