@{
    ViewBag.Title = "जिन्सी समूहको  विवरण";
}
<div class="col-md-12">
    <div class="box box-info">
        <div class="box-body">
            <form class="form-inline">
                <div class="form-group">
                    <label for="Name">जिन्सी समूहको  नाम</label> <br>
                    @Html.TextBox("Name", "", new { @class = "form-control keyword", @style = "width:200px", @placeholder = "जिन्सी समूहको  नाम खोज......" })
                </div>
                <div class="form-group" style="margin-top:25px;">
                    <button type="button" class="btn btn-info btn-reload" data-showLoader="false" data-resultElement=".itemGroup-detail" data-url="@Url.Action("itemgroupdetail","master")"><span class="fa fa-search"></span></button>
                    <a class="btn btn-new btn-info" data-id="0" data-url="@Url.Action("CreateItemGroup", "master")" data-title="Create Item Group"><i class="fa fa-plus" aria-hidden="true"></i></a>
                    <button type="button" class="btn btn-info btnPrint" data-print-element=".itemGroup-detail" data-report-title="Item Group Detail"><span class="fa fa-print"></span></button>
                </div>
            </form>
            <div class="clearfix"></div>
            <div class="itemGroup-detail detail">
                @Html.Action("itemgroupdetail", "master")
            </div>
        </div>
    </div>
</div>
<script>
        initializeCommon();
</script>
